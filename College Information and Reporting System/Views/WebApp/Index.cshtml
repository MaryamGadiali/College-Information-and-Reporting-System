@using College_Information_and_Reporting_System.Models
@using College_Information_and_Reporting_System.Models.ViewModels
@model IndexViewModel

<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/styles.css" />

    <title>Demo page</title>
</head>

<body>

    <!-- Web header -->
    <nav class="navbar">
        <h3 class="webTitle ms-4">School System</h3>
    </nav>

@foreach (var student in Model.Students)
{
        <p>@student.studentFirstName</p>

        var risk = Model.StudentRisks.FirstOrDefault(risk => risk.studentId == student.studentId);


    @* <c:forEach var="c" items="${Cases}"> *@
        <div class="container card col-lg-6 col-sm-12 shadow-sm mt-3 mb-3 p-0">
            <div class="card-header d-flex justify-content-between align-items-center">
                <div>@student.studentFirstName</div>
            </div>
            <div class="card-body">
                <h5 class="card-title ">@student.studentFirstName = @risk.riskStatus</h5>
               
            </div>
          
   
        </div>
   
  }

</body>

</html>